{
  "name": "split_pdfs",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "package": "electron-packager . pdf-to-pages --platform=win32 --arch=x64 --out=dist --overwrite",
    "build": "webpack --config webpack.config.js"
  },
  "build": {
    "appId": "split_pdf",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "cjf.ico"
    },
    "files": [
      "main.js",
      "preload.js",
      "index.html",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "keywords": [],
  "author": "Mtro. José Ángel Haro Juárez",
  "license": "ISC",
  "dependencies": {
    "fs-extra": "^11.2.0",
    "pdf-lib": "^1.17.1"
  },
  "devDependencies": {
    "electron": "^24.2.0",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  }
}
